pipeline {
    agent any
    stages {
        stage('') {
            when {
                expression { sh 'test -e ./prod.go' }
            }
            steps {
                sh './prod.go'
            }
        }
    }
    post{
        success{
            echo 'You can go home'
        }
        failure{
            echo 'Sit and work on'
        }
    }
}
